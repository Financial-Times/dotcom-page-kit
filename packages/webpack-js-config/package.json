{
  "name": "webpack-js-config",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "../../node_modules/.bin/tsc",
    "clean": "rm -rf dist",
    "build:cjs": "npm run tsc -- --outDir ./dist/cjs --module commonjs --target es5 --declarationDir ./dist/dts --declaration true",
    "build:esm": "npm run tsc -- --outDir ./dist/esm --module es2015 --target es2015",
    "build": "npm run clean && npm run build:cjs && npm run build:esm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "babel-loader": "^8.0.4",
    "bower-resolve-webpack-plugin": "^1.0.5"
  },
  "peerDependencies": {
    "webpack": "^4.20.0"
  },
  "engines": {
    "node": ">= 8.12.0"
  }
}
