{
  "name": "@financial-times/anvil",
  "version": "0.0.0",
  "description": "The anvil CLI",
  "main": "dist/cjs/index.js",
  "types": "src/index.ts",
  "bin": {
    "anvil": "bin/cli.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "../../node_modules/.bin/tsc",
